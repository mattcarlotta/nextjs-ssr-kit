{
	"testEnvironment": "jsdom",
	"moduleDirectories": ["<rootDir>/src", "node_modules"],
	"moduleNameMapper": {
		".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/utils/__mocks__/fileMock.js",
		"\\.(css|scss)$": "identity-obj-proxy"
	},
	"transform": {
		"^.+\\.(js|jsx)$": "babel-jest",
		".+\\.(css|styl|less|sass|scss)$": "../node_modules/jest-css-modules-transform"
	},
	"testPathIgnorePatterns": [
		"<rootDir>/.next",
		"<rootDir>/config",
		"<rootDir>/node_modules",
		"<rootDir>/public",
		"<rootDir>/scripts",
		"<rootDir>/server",
		"<rootDir>/babel.config.js",
		"<rootDir>/next.config.js"
	],
	"setupFilesAfterEnv": ["<rootDir>/utils/setupTests/index.js"],
	"moduleFileExtensions": ["css", "scss", "js", "json", "jsx"]
}
