{
  "name": "nextjs-ssr-kit",
  "version": "7.1.0",
  "description": "A Fullstack MERN SSR Boilerplate.",
  "repository": "https://github.com/mattcarlotta/nextjs-ssr-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "analyze": "rimraf .next/analyze && analyze=true npm run build",
    "build": "npm run b:api && concurrently --success first --kill-others \"npm run s:api\" \"npm run b:app\"",
    "b:api": "rimraf build && tsc --p ./api/tsconfig.prod.json",
    "b:app": "ENV_DEBUG=true ENV_LOAD=.env.prod inStaging=false NODE_OPTIONS='-r snackables' next build",
    "b:stage": "ENV_DEBUG=true ENV_LOAD=.env.prod inStaging=true NODE_OPTIONS='-r snackables' next build",
    "b:staging": "npm run seed:prod && npm run b:api && concurrently --success first --kill-others \"npm run s:apistage\" \"npm run b:stage\"",
    "dev": "concurrently  \"npm run d:api\" \"npm run d:app\"",
    "d:app": "ENV_DEBUG=true ENV_LOAD=.env.dev NODE_OPTIONS='--max_old_space_size=4096 --inspect -r snackables' next dev",
    "d:api": "nodemon --config ./api/nodemon.json",
    "drop:dev": "ENV_DEBUG=true ENV_LOAD=.env.teardown,.env.dev ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "drop:prod": "ENV_DEBUG=true ENV_LOAD=.env.teardown,.env.prod ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "drop:stage": "ENV_DEBUG=true ENV_LOAD=.env.teardown,.env.staging ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .ts,.tsx",
    "l:api": "eslint -c ./api/.eslintrc --ignore-path ./api/.eslintignore ./api --ext .ts,.tsx",
    "l:styles": "stylelint **/*.scss",
    "seed:dev": "ENV_DEBUG=true ENV_LOAD=.env.seed,.env.dev ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "seed:prod": "ENV_DEBUG=true ENV_LOAD=.env.seed,.env.prod ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "seed:stage": "ENV_DEBUG=true ENV_LOAD=.env.seed,.env.staging ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "start": "npm run s:api & npm run s:app",
    "s:api": "ENV_DEBUG=true ENV_LOAD=.env.prod node -r ./api/prod-paths.js -r snackables build/api/server.js",
    "s:apistage": "ENV_DEBUG=true ENV_LOAD=.env.staging node -r ./api/prod-paths.js -r snackables build/api/server.js",
    "s:app": "ENV_DEBUG=true ENV_LOAD=.env.prod NODE_OPTIONS='-r snackables' next start",
    "s:staging": "npm run s:apistage & npm run staging",
    "staging": "ENV_DEBUG=true ENV_LOAD=.env.staging NODE_OPTIONS='-r snackables' next start",
    "test": "jest --config ./jest.json --passWithNoTests",
    "t:api": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./api/jest.json --passWithNoTests",
    "t:apicov": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./api/jest.json --coverage",
    "t:apiwatch": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./api/jest.json --watchAll",
    "t:cov": "jest --config ./jest.json --coverage",
    "t:e2e": "npm run b:staging && concurrently --success first --kill-others \"npm run s:staging\" \"cypress open --project ./e2e\"",
    "t:e2erun": "npm run b:staging && concurrently --success first --kill-others \"npm run s:staging\" \"cypress run --config video=false --project ./e2e\"",
    "t:stage": "npm run b:staging && npm run s:staging",
    "t:failed": "jest --config ./jest.json --watch --onlyFailures",
    "t:watch": "jest --config ./jest.json --watch --onlyChanged",
    "t:watchall": "jest --config ./jest.json --watchAll",
    "tsc": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "mongoose": "^5.12.8",
    "morgan": "^1.10.0",
    "next": "^10.2.0",
    "next-redux-wrapper": "^6.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "snackables": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@emotion/jest": "^11.3.0",
    "@types/bluebird": "^3.5.34",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/react-dom": "^17.0.4",
    "@types/react-redux": "^7.1.16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.16.6",
    "concurrently": "^6.1.0",
    "cypress": "^7.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.1",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.5.3",
    "nodemon": "^2.0.7",
    "opener": "^1.5.2",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga-test-plan": "^4.0.1",
    "sass": "^1.32.13",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-scss": "^3.19.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
