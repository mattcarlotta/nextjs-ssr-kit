{
	"name": "nextjs-ssr-kit",
	"version": "1.0.0",
	"description": "A fullstack MERN SSR boilerplate.",
	"repository": "https://github.com/mattcarlotta/nextjs-ssr-kit",
	"author": "Matt Carlotta",
	"license": "MIT",
	"private": false,
	"scripts": {
		"analyze": "rimraf .next/analyze && analyze=true npm run build",
		"build": "NODE_ENV=production next build",
		"checkbuild": "es-check es5 './.next/static/**/*.js'",
		"dev": "NODE_env=development next dev",
		"lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .js,.jsx",
		"lint:styles": "stylelint **/*.scss",
		"start": "NODE_ENV=production next start -p 8080",
		"test": "NODE_ENV=test jest --config ./jest.json --passWithNoTests",
		"test:cov": "NODE_ENV=test jest --config ./jest.json --coverage",
		"test:watch": "NODE_ENV=test jest --config ./jest.json --watch --onlyChanged",
		"test:watchall": "NODE_ENV=test jest --config ./jest.json --watchAll"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"npm run lint",
			"npm run test"
		],
		"*.scss": [
			"npm run lint:styles"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"axios": "^0.19.0",
		"bluebird": "^3.7.2",
		"body-parser": "^1.19.0",
		"chalk": "^3.0.0",
		"compression": "^1.7.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"fs-extra": "^8.1.0",
		"lodash": "^4.17.15",
		"mini-css-extract-plugin": "^0.9.0",
		"moment-timezone": "^0.5.27",
		"mongoose": "^5.8.4",
		"morgan": "^1.9.1",
		"next": "9.1.6",
		"next-redux-saga": "^4.1.2",
		"next-redux-wrapper": "^4.0.1",
		"pify": "^4.0.1",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-icons": "^3.8.0",
		"react-redux": "^7.1.3",
		"react-toastify": "^5.5.0",
		"redux": "^4.0.5",
		"redux-saga": "^1.1.3",
		"styled-components": "^4.4.1",
		"webpack-bundle-analyzer": "^3.6.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.7",
		"@babel/core": "^7.7.7",
		"@babel/node": "^7.7.7",
		"@zeit/next-css": "^1.0.1",
		"@zeit/next-sass": "^1.0.1",
		"autoprefixer": "^9.7.3",
		"axios-mock-adapter": "^1.17.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^24.9.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-styled-components": "^1.10.6",
		"browserslist": "^4.8.3",
		"cross-env": "^6.0.3",
		"css-loader": "^3.4.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"es-check": "^5.1.0",
		"eslint": "^6.7.2",
		"eslint-config-airbnb": "^18.0.0",
		"eslint-config-airbnb-base": "^13.2.0",
		"eslint-config-prettier": "^6.7.0",
		"eslint-import-resolver-babel-module": "^5.1.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-jest": "^23.1.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"eslint-watch": "^6.0.1",
		"extracted-loader": "^1.0.7",
		"file-loader": "^5.0.2",
		"husky": "^3.1.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.9.0",
		"jest-css-modules-transform": "^3.1.0",
		"jest-enzyme": "^7.1.2",
		"jest-haste-map": "^24.9.0",
		"jest-resolve": "^24.9.0",
		"jest-styled-components": "^6.3.4",
		"jsdom": "^15.2.1",
		"lint-staged": "^9.5.0",
		"node-sass": "^4.13.0",
		"nodemon": "^2.0.2",
		"postcss-flexbugs-fixes": "^4.1.0",
		"postcss-loader": "^3.0.0",
		"postcss-normalize": "^8.0.1",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^1.19.1",
		"prop-types": "^15.7.2",
		"redux-devtools-extension": "^2.13.8",
		"redux-saga-test-plan": "^4.0.0-rc.3",
		"sass-loader": "^8.0.0",
		"stylelint": "^12.0.0",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-scss": "^3.13.0",
		"supertest": "^4.0.2",
		"url-loader": "^3.0.0",
		"webpack": "^4.41.5"
	}
}
